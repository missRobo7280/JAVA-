//Enhanced for loop also called the fora each loop
class forEach
{
    public static void main(String[] args)
    {
        int a[]=new int[4];
        a[0]=5;
        a[1]=6;
        a[2]=7;
        a[3]=10;
        for(int i:a)
        {
            
            System.out.println(i);
        }
    }
}
