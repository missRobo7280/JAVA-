//switch statement in JAVA
import java.util.Scanner;
class Switch_Statement
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int day=sc.nextInt();
        switch(day)
        {
            case 1:
            System.out.println("Monday----Setting alarm for 5:30");
            break;
            case 2:
            System.out.println("Tuesday----Setting alarm for 5:15");
            break;
            case 3:
            System.out.println("Wednesday----Setting an alarm for 5:00");
            break;
            case 4:
            System.out.println("Thursday-----Setting an alarm for 5:15");
            break;
            case 5:
            System.out.println("Friday----Setting an alarm for 6:00");
            break;
            case 6:
            System.out.println("Saturday----Setting an alarm for 5:45");
            break;
            case 7:
            System.out.println("Sunday----Setting an alarm for 7:00");
            break;
            default:
            System.out.println("Sleep today");


        }
    }
}
